<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="registration_requirements_form" model="ir.ui.view">
         <field name="name">registration.requirements.wizard</field>
         <field name="model">registration.requirements</field>
         <field name="arch" type="xml">
            <form string="registration requirements">
                <group>
                    <field name="registration_counter"/>
                    <field name="registration_staff"/>
                </group>
            </form>
         </field>
     </record>

      <record id="registration_requirements_wizard" model="ir.actions.act_window">
        <field name="name">registration requirements</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">registration.requirements</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="registration_requirements_form"/>
        <field name="target">new</field>
    </record>

</odoo>
