<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="event_photobooth_form" model="ir.ui.view">
         <field name="name">event.photobooth.wizard</field>
         <field name="model">event.photobooth</field>
         <field name="arch" type="xml">
            <form string="event photobooth">
                <group>
                    <field name="photobooth_backdrop"/>
                    <field name="photobooth_images"/>
                    <field name="photobooth_gif"/>
                    <field name="glambot_photobooth"/>
                </group>
            </form>
         </field>
     </record>

      <record id="event_photobooth_wizard" model="ir.actions.act_window">
        <field name="name">event photobooth</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">event.photobooth</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="event_photobooth_form"/>
        <field name="target">new</field>
    </record>

</odoo>
