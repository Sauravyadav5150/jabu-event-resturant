<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="design_requirements_form" model="ir.ui.view">
         <field name="name">design.requirements.wizard</field>
         <field name="model">design.requirements</field>
         <field name="arch" type="xml">
            <form string="design Requirements">
                <group>
                    <field name="artwork"/>
                    <field name="design"/>
                </group>
            </form>
         </field>
     </record>

      <record id="design_requirements_wizard" model="ir.actions.act_window">
        <field name="name">design requirements</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">design.requirements</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="design_requirements_form"/>
        <field name="target">new</field>
    </record>

</odoo>
