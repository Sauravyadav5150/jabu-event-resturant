<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="event_manpower_form" model="ir.ui.view">
         <field name="name">event.manpower.wizard</field>
         <field name="model">event.manpower</field>
         <field name="arch" type="xml">
            <form string="event manpower">
                <group>
                    <field name="emcee"/>
                    <field name="hostesses"/>
                    <field name="ushers"/>
                    <field name="models"/>
                    <field name="security"/>
                    <field name="event_staff"/>
                    <field name="interpreter"/>
                    <field name="labor"/>
                    <field name="delivery"/>
                </group>
            </form>
         </field>
     </record>

      <record id="event_manpower_wizard" model="ir.actions.act_window">
        <field name="name">event manpower</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">event.manpower</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="event_manpower_form"/>
        <field name="target">new</field>
    </record>

</odoo>
