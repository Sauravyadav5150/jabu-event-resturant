<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="event_entertainment_form" model="ir.ui.view">
         <field name="name">event.entertainment.wizard</field>
         <field name="model">event.entertainment</field>
         <field name="arch" type="xml">
            <form string="event entertainment">
                <group>
                    <field name="thai_theme"/>
                    <field name="tech_led"/>
                    <field name="band"/>
                    <field name="dance"/>
                    <field name="ambient"/>
                    <field name="cirque"/>
                </group>
            </form>
         </field>
     </record>

      <record id="event_entertainment_wizard" model="ir.actions.act_window">
        <field name="name">event entertainment</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">event.entertainment</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="event_entertainment_form"/>
        <field name="target">new</field>
    </record>

</odoo>
